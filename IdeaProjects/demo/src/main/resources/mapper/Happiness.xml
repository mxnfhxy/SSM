<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.HappinessDao">
    <select id="findHappinessByCityMXN" parameterType="String" resultType="com.example.demo.domain.Happiness">
        SELECT * FROM happiness WHERE city = #{city}
    </select>

    <insert id="insertHappinessMXN" parameterType="HashMap" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO happiness(city, num) VALUES(#{city}, #{num})
    </insert>
</mapper>
